<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.shaden.funtolearn.MainActivity"
    android:background="#FFFF">
    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridLayout"
        android:columnCount="5"
        android:rowCount="6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_1"
            android:src="@drawable/image_1"
            android:layout_row="0"
            android:layout_column="0" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_2"
            android:src="@drawable/image_2"
            android:layout_row="0"
            android:layout_column="1" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_3"
            android:src="@drawable/image_3"
            android:layout_row="0"
            android:layout_column="2" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_4"
            android:src="@drawable/image_4"
            android:layout_row="0"
            android:layout_column="3" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_5"
            android:src="@drawable/image_5"
            android:layout_row="0"
            android:layout_column="4" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_6"
            android:src="@drawable/image_6"
            android:layout_row="1"
            android:layout_column="0" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_7"
            android:src="@drawable/image_7"
            android:layout_row="1"
            android:layout_column="1" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_8"
            android:src="@drawable/image_8"
            android:layout_row="1"
            android:layout_column="2" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_9"
            android:src="@drawable/image_9"
            android:layout_row="1"
            android:layout_column="3" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_10"
            android:src="@drawable/image_10"
            android:layout_row="1"
            android:layout_column="4" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_11"
            android:src="@drawable/image_11"
            android:layout_row="2"
            android:layout_column="0" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_12"
            android:src="@drawable/image_12"
            android:layout_row="2"
            android:layout_column="1" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_13"
            android:src="@drawable/image_13"
            android:layout_row="2"
            android:layout_column="2" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_14"
            android:src="@drawable/image_14"
            android:layout_row="2"
            android:layout_column="3" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/i_15"
            android:src="@drawable/image_15"
